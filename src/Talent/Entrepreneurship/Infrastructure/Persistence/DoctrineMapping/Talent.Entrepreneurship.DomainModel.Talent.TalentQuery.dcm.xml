            table='bkp_talent'
            repository-class='Talent\Entrepreneurship\Infrastructure\DomainModelRepository\Talent\DoctrineTalentQueryRepository'
    >
        
        <one-to-many field='entrepreneurshipRdos' 
                     target-entity='Talent\Entrepreneurship\DomainModel\Entrepreneurship\EntrepreneurshipRdo'
                     mapped-by='talentQuery'
